        <file role="data" name="app.yml"/>
        <file role="data" name="config.php"/>
        <file role="data" name="module.yml"/>
      </dir>
      <dir name="lib">
        <dir name="filter">
          <file role="data" name="sfGoogleAnalyticsFilter.class.php"/>
        </dir>
        <dir name="helper">
          <file role="data" name="GoogleAnalyticsHelper.php"/>
        </dir>
        <dir name="listener">
          <file role="data" name="sfGoogleAnalyticsListener.class.php"/>
        </dir>
        <dir name="mixin">
          <file role="data" name="sfGoogleAnalyticsMixin.class.php"/>
        </dir>
        <dir name="tracker">
          <file role="data" name="sfGoogleAnalyticsTracker.class.php"/>
          <file role="data" name="sfGoogleAnalyticsTrackerGoogle.class.php"/>
          <file role="data" name="sfGoogleAnalyticsTrackerUrchin.class.php"/>
        </dir>
        <dir name="transaction">
          <file role="data" name="sfGoogleAnalyticsItem.class.php"/>
          <file role="data" name="sfGoogleAnalyticsTransaction.class.php"/>
        </dir>
        <dir name="util">
          <file role="data" name="sfGoogleAnalyticsToolkit.class.php"/>
        </dir>
      </dir>
      <file role="data" name="LICENSE"/>
      <file role="data" name="README"/>
