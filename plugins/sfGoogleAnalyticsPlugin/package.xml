  <lead>
    <name>Kris Wallsmith</name>
    <user>Kris.Wallsmith</user>
    <email>kris.wallsmith@symfony-project.com</email>
    <active>yes</active>
  </lead>
  <date>2008-10-17</date>
  <version>
    <release>1.1.5</release>
    <api>1.1.5</api>
  </version>
  <stability>
    <release>stable</release>
    <api>stable</api>
  </stability>
  <license uri="http://www.symfony-project.com/license">MIT license</license>
  <notes>-</notes>
  <contents>
    <dir name="/">
